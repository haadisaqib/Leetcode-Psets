class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        
        str_num = ''.join(str(num) for num in digits)

        res_int = int(str_num) + 1

        res_string = str(res_int)

        return [int(digit) for digit in res_string]

